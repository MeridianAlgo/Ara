{
  "api": {
    "port": 3000,
    "host": "localhost"
  },
  "database": {
    "path": "./data/stock_data.db"
  },
  "model": {
    "save_path": "./data/models/",
    "hidden_layers": [64, 32, 16],
    "dropout_rate": 0.2,
    "learning_rate": 0.001,
    "batch_size": 32,
    "epochs": 100
  },
  "indicators": {
    "rsi_period": 14,
    "macd_fast": 12,
    "macd_slow": 26,
    "macd_signal": 9,
    "sma_periods": [5, 10, 20, 50],
    "ema_periods": [12, 26],
    "bollinger_period": 20,
    "bollinger_std": 2
  },
  "data_sources": {
    "primary": "yfinance",
    "fallback": "alpha_vantage",
    "cache_duration": 300
  },
  "logging": {
    "level": "info",
    "file": "./logs/app.log",
    "max_size": "10MB",
    "max_files": 5
  }
}